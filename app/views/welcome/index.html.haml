.span6
  .well
    = simple_form_for @user, url: user_session_path, html: {class: 'form-horizontal sign_in_form'} do |f|
      = f.input :username
      = f.input :password
      = f.submit 'Sign In', class: 'btn btn-primary'
.span6.align_right
  = image_tag 'logo.png'
  %h4 Show Your Bidding Skill
